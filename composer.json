{
  "require": {
      "yahnis-elsts/plugin-update-checker": "^4.8",
      "aristath/kirki": "^3.0",
      "timber/timber": "^1.12",
      "jjgrainger/posttypes": "^2.0",
      "monolog/monolog": "^2.0",
      "nesbot/carbon": "^2.26",
      "htmlburger/carbon-fields": "^3.1",
      "dusank/knapsack": "^10.0"
  },
  "require-dev": {
        "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "vendor/aristath/kirki/kirki.php",
      "src/app.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
